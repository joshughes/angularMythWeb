div(ng-app='ng-app')
  h2 Vidoes
  div(class='container-fluid')
    div(ng-controller='VideoCtrl')
      span THis 
      ul.unstyled
        li(ng-repeat='video in videos.VideoMetadataInfoList.VideoMetadataInfos')
          div(class="row-fluid")
            img(src='{{video.Artwork.ArtworkInfos[1].URL}}&Width=200',class="span3")
            div(class='span9')
              h4 
                a(ng-href='#/video/{{video.Id}}') {{video.Title}} {{video.Certification}}
              p {{video.Description}}
        
  